        <fullName>Submission_Date_Validation</fullName>
        <active>true</active>
        <description>This is a validation rule to restrict user from entering a  submission date less than proposal assignment date.</description>
        <errorConditionFormula>Target_Submission_Date__c  &lt;  Proposal_Assignment_Date__c</errorConditionFormula>
